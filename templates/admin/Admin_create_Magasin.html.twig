{% extends 'base.html.twig' %}

{% block title %}Support Cash Converters - Admin NewMagasin{% endblock %}

{% block body %}

    <div class="card card_ticket">
        <div class="card-header">
            <h3> <i class="fas fa-plus"> Création Magasin  </i></h3>

        </div>
        <div class="card-body">
            <div class="col-md-12 .offset-md-12">
                <a href="{{ path('admin.mag.all') }}" class="btn btn-outline-dark" role="button"><i class="fas fa-undo"> Retour à la liste des Magasins </i> </a>
            </div>
            <br>
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <button type="submit" role="button" class="btn btn-outline-success"> <i class="fas fa-plus"> Créer </i></button>
            {{form_end(form)}}
        </div>
    </div>
    <br>
{% endblock %}