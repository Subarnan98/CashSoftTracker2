{% extends 'base.html.twig' %}
{% block title %}Support Cash Converters - Admin NewMessage {% endblock %}
{% block body %}
    <div class="card card_ticket">
        <div class="card-header">
            <h3> <i class="fas fa-plus"> Création Message support  </i></h3>

        </div>
        <div class="card-body">
            <br>
            <form method="post" action="" class="needs-validation" novalidate>
                {{ form_start(form) }}
                <div class="my-custom-class-for-errors">
                    {{ form_errors(form) }}
                </div>
                {{ form_widget(form) }}
                <button type="submit" role="button" class="btn btn-outline-success"> <i class="fas fa-plus"> Créer </i></button>
                {{form_end(form)}}
            </form>
        </div>
    </div>
    <br>
{% endblock %}